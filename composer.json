{
    "name": "datlv/laravel-menu",
    "description": "Laravel Menu Package",
    "keywords": [
        "datlv",
        "laravel",
        "menu"
    ],
    "homepage": "https://github.com/datlv/laravel-menu",
    "license": "MIT",
    "authors": [
        {
            "name": "Datlv",
            "email": "datle2011@gmail.com",
            "homepage": "http://datlv.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "baum/baum": "~1.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Datlv\\Menu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datlv\\Menu\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Datlv\\Menu\\ServiceProvider"
            ]
        }
    }
}
